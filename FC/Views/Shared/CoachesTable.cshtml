@model IEnumerable<FC.Models.Coach>
<tbody>
 <tr>
        <th>
           Ім'я
        </th>
        <th>
            Прізвище
        </th>
        <th>
           Дата народження
        </th>
        <th>
           Команда
        </th>
        <th></th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CoachName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CoachSurname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BirthdayDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Team.TeamName)
        </td>
        <td>
            @Html.ActionLink("Редагувати", "EditCoach", new {controller="Coaches", id = item.CoachId}) |
            @Html.ActionLink("Деталі", "DetailsOfCoach", new {controller="Coaches",id = item.CoachId}) |
            @Html.ActionLink("Видалити", "DeleteCoach", new {controller="Coaches",id = item.CoachId})
        </td>
    </tr>
}
</tbody>